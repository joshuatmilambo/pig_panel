{
  "name" : "Pig Panel",
  "description" : "Base Level Extension",
  "version" : "1.0",
  "manifest_version" : 3,
  "action" : {
    "default_popup" : "hello.html",
    "default_icon" : "hello_extension.png"
  },
  "host_permissions": ["https://fantasy.afl.com.au/*"],
  "content_scripts" : [
    {
      "matches": ["https://fantasy.afl.com.au/*"],
      "js": ["scripts/content.js"],
      "css": ["styles.css"]
    }
  ]
}